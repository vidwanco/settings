{
    "name": "vidwanco/settings",
    "description": "Database Driven Dynamic Settings with Form Builder",
    "keywords": [
        "vidwanco",
        "settings"
    ],
    "homepage": "https://github.com/vidwanco/settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Shashwat Mishra",
            "email": "secrethash96@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Vidwanco\\Settings\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vidwanco\\Settings\\SettingsServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vidwanco\\Settings\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
